<div class="container">
<form>
  <h3>Booking for Servicing</h3>
  <div class="col-md-5">
    <div class="row"> 
      <div class="form-group">
        <label>Name</label>
        <input type="text" class="form-control">
      </div>
    </div>
    <div class="row"> 
      <div class="form-group">
        <label>age</label>
        <input type="text" class="form-control">
      </div>
    </div>
    <div class="row"> 
      <div class="form-group">
        <label>City</label>
        <input type="text" class="form-control">
      </div>
    </div>
    <div class="row"> 
      <div class="form-group">
        <label>Date</label>
        <input type="text" class="form-control">
      </div>
    </div>
    <div class="row"> 
      <div class="form-group">
        <label>Mobile No</label>
        <input type="text" class="form-control">
      </div>
    </div>
    <div class="row"> 
      <div class="form-group">
          <label>Model:</label>           
          <select name="model" size="1" class="input1" id="model"> 
            <option value="select" selected="">Select</option>
            <option>CT 100</option>
            <option>PLATINA </option>
            <option>Discover 125 </option>
            <option>V 12 </option>
            <option>V 15</option>
            <option>PULSAR 135 LS</option>
            <option>PULSAR 150</option>
            <option>PULSAR 180</option>
            <option>PULSAR 220</option>
            <option>PULSAR 200 NS</option>
            <option>PULSAR 220 RS</option> 
            <option>AVENGER 150 STREET</option>
            <option>AVENGER 220 CRUISE</option>
            <option>AVENGER 220 STREET</option>
            <option>DOMINAR</option>
          </select>
        </div>
      </div>
   
    <div class="row">
      <div class="form-group">
        <button class="btn btn-primary btn-block form-control">Submit</button>
      </div>
    </div>
  </div>

  <div class="col-md-7">
    <%= image_tag "bike-servicing.jpg", class: "img-responsive" %>
  </div>
</form>
</div>

